{
  "name": "lan/meetup-boomstick",
  "description": "PHP Module for generate JSON structure of HTML forms and control elements for REST-API requests",
  "minimum-stability": "stable",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Emil Limarenko",
      "email": "ash_williams@mail.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "Metup\\Boomstick\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Metup\\Boomstick\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "7.0.* || 7.1.* || 7.2.* || 7.3.* || 7.4.* || 8.0.*"
  },
  "require-dev": {
    "codeception/codeception": "^5.0",
    "codeception/module-asserts": "*",
    "codeception/module-phpbrowser": "*"
  },
  "config": {
    "secure-http": false,
    "sort-packages": true,
    "platform": {
      "php": "8.0"
    }
  }
}
